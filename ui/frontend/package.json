    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "axios": "^0.20.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
